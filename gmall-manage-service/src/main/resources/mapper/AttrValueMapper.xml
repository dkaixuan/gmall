<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.gmall.manage.mapper.AttrValueMapper">

    <insert id="insertByAttrId">
        insert into pms_base_attr_value(value_name,attr_id) values
        <foreach collection="attrValueList" separator="," item="item">
            (
            #{item.valueName},#{attrId}
            )
        </foreach>
    </insert>


</mapper>